{
  "comment": {
    "prefix": "cmt",
    "body": ["{/* ", "\t$0", "*/}"],
    "description": "JSX comment"
  },
  "note": {
    "prefix": "nt",
    "body": ["{/*NOTE: $0 */}"],
    "description": "note"
  },
  "fixme": {
    "prefix": "fix",
    "body": ["{/*FIXME: $0 */}"],
    "description": "fixme"
  },
  "doc": {
    "prefix": "doc",
    "body": ["/**", " * $0", " */"],
    "description": "JSDoc comment block with params and returns"
  }
}
